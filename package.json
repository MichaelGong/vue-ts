{
  "name": "lab.happybug.top",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:report": "vue-cli-service build --report",
    "start": "vue-cli-service serve --open",
    "sentry:map": "sentry-cli releases -o sentry -p vuelab files product@0.0.2 upload-sourcemaps ./dist/js --url-prefix https://lab.happybug.top/js/ --rewrite",
    "sentry:del": "sentry-cli releases -o sentry -p vuelab delete product@0.0.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "element-ui": "^2.4.5",
    "lodash": "^4.17.11",
    "qs": "^6.5.2",
    "raven-js": "^3.27.0",
    "register-service-worker": "^1.0.0",
    "ress": "^1.2.2",
    "vue": "^2.5.17",
    "vue-canvas-nest": "^2.0.0",
    "vue-class-component": "^6.0.0",
    "vue-grid-layout": "^2.2.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.117",
    "@types/qs": "^6.5.1",
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-pwa": "^3.0.4",
    "@vue/cli-plugin-typescript": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-lodash": "^3.3.4",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.2.2",
    "lodash-webpack-plugin": "^0.11.5",
    "style-resources-loader": "^1.2.1",
    "typescript": "^3.0.0",
    "vue-cli-plugin-element": "^1.0.0",
    "vue-cli-plugin-style-resources-loader": "^0.1.3",
    "vue-template-compiler": "^2.5.17",
    "webpack-sentry-plugin": "^1.16.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
